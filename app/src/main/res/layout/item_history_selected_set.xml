<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="48dp">

    <TextView
        style="@style/BlackTextBodySecondary"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Set "
        android:id="@+id/textView16"
        android:layout_marginLeft="72dp"
        android:layout_centerVertical="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0"
        android:id="@+id/set_number_textview"
        android:layout_alignTop="@+id/textView16"
        android:layout_toEndOf="@+id/textView16" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="10"
        android:id="@+id/reps_text_view"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="x"
        android:id="@+id/textView18"
        android:layout_alignTop="@+id/reps_text_view"
        android:layout_toEndOf="@+id/reps_text_view" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="100"
        android:id="@+id/weight_text_view"
        android:layout_alignBaseline="@+id/textView18"
        android:layout_alignBottom="@+id/textView18"
        android:layout_toEndOf="@+id/textView18"
        android:layout_marginLeft="8dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="lb"
        android:id="@+id/weight_unit_name_text_view"
        android:layout_alignTop="@+id/weight_text_view"
        android:layout_toEndOf="@+id/weight_text_view"
        android:layout_marginLeft="3dp" />

    <TextView
        style="@style/BlackTextBodyHint"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="12:30 A.M."
        android:id="@+id/set_time_stamp_text_view"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_alignBaseline="@+id/weight_unit_name_text_view"
        android:layout_alignBottom="@+id/weight_unit_name_text_view"
        android:layout_alignParentEnd="true" />

</RelativeLayout>