<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="48dp">

    <TextView
        android:id="@+id/set_text_view"
        style="@style/BlackTextBodyPrimary"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_centerVertical="true"
        android:text="Set"
        android:layout_marginLeft="72dp"
        android:layout_marginRight="3dp"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="16dp" />

    <TextView
        android:id="@+id/set_number_text_view"
        style="@style/BlackTextBodyPrimary"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0"
        android:layout_alignBaseline="@+id/set_text_view"
        android:layout_alignBottom="@+id/set_text_view"
        android:layout_toEndOf="@+id/set_text_view" />

    <TextView
        style="@style/BlackTextBodySecondary"
        android:id="@+id/reps_number_text_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0"
        android:layout_marginLeft="72dp"
        android:layout_marginRight="3dp"
        android:layout_centerVertical="true"
        android:layout_toEndOf="@+id/set_number_text_view"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="16dp" />

    <TextView
        style="@style/BlackTextBodySecondary"
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Reps"
        android:layout_centerVertical="true"
        android:layout_toEndOf="@+id/reps_number_text_view" />

</RelativeLayout>