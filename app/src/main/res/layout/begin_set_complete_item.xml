<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:padding="6dp">

    <TextView
        android:id="@+id/set_text_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="33dp"
        android:paddingBottom="5dp"
        android:paddingLeft="10dp"
        android:paddingRight="2dp"
        android:paddingTop="5dp"
        android:text="Set"
        android:textColor="@color/textLightGrey"
        android:textSize="15sp" />

    <TextView
        android:id="@+id/set_number_complete_textv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_toEndOf="@+id/set_text_view"
        android:paddingBottom="5dp"
        android:paddingLeft="2dp"
        android:paddingRight="10dp"
        android:paddingTop="5dp"
        android:text="0"
        android:textColor="@color/textLightGrey"
        android:textSize="15sp" />

    <TextView
        android:id="@+id/rep_number_complete_textv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:paddingBottom="5dp"
        android:paddingLeft="10dp"
        android:paddingRight="2dp"
        android:paddingTop="5dp"
        android:text="0"
        android:textColor="@color/textLightGrey"
        android:textSize="15sp" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_toEndOf="@+id/rep_number_complete_textv"
        android:paddingBottom="5dp"
        android:paddingLeft="2dp"
        android:paddingRight="2dp"
        android:paddingTop="5dp"
        android:text="Reps,"
        android:textColor="@color/textLightGrey"
        android:textSize="15sp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_toEndOf="@+id/weight_number_complete_textv"
        android:paddingBottom="5dp"
        android:paddingLeft="2dp"
        android:paddingRight="10dp"
        android:paddingTop="5dp"
        android:text="Lbs"
        android:textColor="@color/textLightGrey"
        android:textSize="15sp" />

    <TextView
        android:id="@+id/weight_number_complete_textv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_toEndOf="@+id/textView11"
        android:paddingBottom="5dp"
        android:paddingLeft="2dp"
        android:paddingRight="2dp"
        android:paddingTop="5dp"
        android:text="0"
        android:textColor="@color/textLightGrey"
        android:textSize="15sp" />

</RelativeLayout>